# Database
DATABASE_URL=postgresql://leo:leo_password@db:5432/leo

# LLM API Keys (at least one required)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# LiteLLM
LITELLM_URL=http://litellm:4000

# ChromaDB
CHROMA_URL=http://chroma:8000

# Embeddings
EMBEDDING_MODEL=text-embedding-3-small

# Default LLM Model (используется для генерации промптов и т.д.)
# Доступные: claude-3-haiku-20240307, claude-3-5-sonnet-20241022, gpt-4o, gpt-4o-mini
DEFAULT_LLM_MODEL=claude-3-haiku-20240307

# LangSmith (for tracing)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=ls-...
LANGCHAIN_PROJECT=leo-agents

# Agent defaults
AGENT_IMAGE=leo-agent-runtime:latest
GATEWAY_URL=http://leo-gateway:8080
DOCKER_NETWORK=leo_network

